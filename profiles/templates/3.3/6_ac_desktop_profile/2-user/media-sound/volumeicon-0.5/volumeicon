# Calculate format=kde path=~/.config/volumeicon

[StatusIcon]
stepsize=5
lmb_slider=true
mmb_mute=false
use_horizontal_slider=false
show_sound_level=false
onclick=bash -c "/usr/bin/alsamixer"
theme=White Gnome

[Hotkeys]
up_enabled=true
down_enabled=true
mute_enabled=true
up=XF86AudioRaiseVolume
down=XF86AudioLowerVolume
mute=XF86AudioMute

[Alsa]
card=default
