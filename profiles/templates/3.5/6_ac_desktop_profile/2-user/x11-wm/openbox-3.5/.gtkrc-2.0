# Calculate pkg(x11-themes/gnome-themes-standard)!=
# include standart themes
#?pkg(x11-themes/clearlooks-phenix)!=#
gtk-theme-name="Clearlooks-Phenix"
#!pkg#
gtk-theme-name="Calculate"
#pkg#
gtk-icon-theme-name="Calculate"
gtk-font-name="Droid Sans 10"
gtk-cursor-theme-name="haematite"
gtk-menu-images=1
gtk-button-images=1
gtk-toolbar-style=GTK_TOOLBAR_BOTH
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="hintfull"
gtk-xft-rgba="rgb"
#?pkg(x11-themes/elementary-icon-theme)!=#
gtk-icon-theme-name="elementary"
#pkg#

# include personal settings
include "/home/#-ur_login-#/.gtkrc-2.0-personal"
